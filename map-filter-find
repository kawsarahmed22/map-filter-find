const oddNumbers = [1, 3, 5, 7,9];
oddNumbers.map(function(element, index, array){
    console.log(element, index, array);
})

// Find out Squares of Numbers using map
const numbers = [2, 4, 6, 8];
const squares = numbers.map(number => number * number);
console.log(squares);

// filter
const bigger = numbers.filter(number => number > 5);
console.log(bigger);

// find
const FirstBiggerNumber = numbers.find(number => number > 5);
console.log(FirstBiggerNumber);